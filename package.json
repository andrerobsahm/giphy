{
  "name": "giphy",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "browser": "browser-sync start --server public --files public/**/*",
    "build:styles": "node-sass --source-map true --output-style compressed src/styles/giphy.scss public/assets/styles/output.css",
    "watch:styles": "node-sass src/styles/giphy.scss --watch public/assets/styles/output.css",
    "watch": "npm-run-all --parallel browser watch:*"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "browser-sync": "^2.23.6",
    "node-sass": "^4.7.2",
    "npm-run-all": "^4.1.2",
    "query-string": "^5.0.1",
    "sanitize.css": "^5.0.0"
  }
}
